{
    "name": "viewx-vscode",
    "displayName": "viewX-vscode",
    "description": "Extension for visualizing DSL code structure written using textX.",
    "version": "0.0.1",
    "publisher": "dkupco",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:viewx.setActiveModel",
        "onCommand:viewx.viewModel"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "viewx.setActiveModel",
            "title": "viewX: Set active viewX model"
        },
        {
            "command": "viewx.viewModel",
            "title": "viewX: View model"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}