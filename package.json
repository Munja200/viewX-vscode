{
    "name": "viewx-vscode",
    "displayName": "viewX-vscode",
    "description": "Extension for visualizing DSL code structure written using textX.",
    "version": "0.0.1",
    "publisher": "dkupco",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:viewx.setActiveModel",
        "onCommand:viewx.viewModel",
        "onCommand:viewx.initProject"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "viewx.setActiveModel",
            "title": "viewX: Set active viewX model"
        },
        {
            "command": "viewx.viewModel",
            "title": "viewX: View model"
        },
        {
            "command": "viewx.initProject",
            "title": "viewX: Initialize viewX project"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.4.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^8.0.24",
        "@types/mocha": "^2.2.42"
    }
}